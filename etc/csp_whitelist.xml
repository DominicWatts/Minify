<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="apptrian-script-src" type="host">www.apptrian.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="apptrian-connect-src" type="host">www.apptrian.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="apptrian-img-src" type="host">www.apptrian.com</value>
            </values>
        </policy>
        <policy id="media-src">
            <values>
                <value id="apptrian-media-src" type="host">www.apptrian.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
